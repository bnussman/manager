{
  "name": "@linode/api-v4",
  "version": "0.66.0",
  "homepage": "https://github.com/linode/manager/tree/develop/packages/api-v4",
  "bugs": {
    "url": "https://github.com/linode/manager/issues",
    "email": "feedback@linode.com"
  },
  "repository": {
    "type": "git",
    "directory": "https://github.com/linode/manager/tree/develop/packages/api-v4"
  },
  "engines": {
    "node": ">= 14.17.4"
  },
  "description": "JavaScript wrapper around the Linode APIv4",
  "author": "Linode",
  "license": "Apache-2.0",
  "private": false,
  "main": "dist/api-v4.umd.js",
  "unpkg": "dist/api-v4.iife.js",
  "jsdelivr": "dist/api-v4.iife.js",
  "module": "dist/api-v4.es.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/api-v4.es.js",
      "require": "./dist/api-v4.umd.js"
    }
  },
  "dependencies": {
    "@linode/validation": "0.8.0",
    "@types/yup": "^0.29.13",
    "axios": "~0.21.4",
    "ipaddr.js": "^2.0.0",
    "querystring": "^0.2.0",
    "yup": "^0.32.9"
  },
  "scripts": {
    "dev": "yarn start",
    "build": "vite build && tsc --emitDeclarationOnly && mv dist/src dist/types",
    "start": "concurrently --raw \"tsc -w --preserveWatchOutput\" \"vite --clearScreen false\"",
    "test": "jest --config jestconfig.json",
    "lint": "yarn run eslint . --quiet --ext .js,.ts,.tsx",
    "typecheck": "tsc --noEmit true --emitDeclarationOnly false",
    "precommit": "lint-staged"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/jest": "^26.0.13",
    "@types/node": "^12.7.1",
    "axios-mock-adapter": "^1.18.1",
    "concurrently": "^4.1.1",
    "eslint": "^6.8.0",
    "eslint-plugin-ramda": "^2.5.1",
    "eslint-plugin-sonarjs": "^0.5.0",
    "jest": "~26.4.2",
    "lint-staged": "^9.4.2",
    "prettier": "~2.2.1",
    "ts-jest": "^24.1.0",
    "vite": "^2.8.6"
  },
  "resolutions": {
    "handlebars": "^4.4.3"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "prettier --write",
      "eslint --ext .js,.ts,.tsx",
      "git add"
    ],
    ".{ts,tsx}": [
      "tsc -p tsconfig.json --noEmit true --emitDeclarationOnly false"
    ]
  }
}
